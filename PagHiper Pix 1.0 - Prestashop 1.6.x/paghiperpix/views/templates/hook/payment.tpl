<!-- javascript e css comum a todos -->
<link rel="stylesheet" href="{$this_path_ssl}views/css/front.css" type="text/css" media="all" />
<script>
var baseDir = '{$url_loja}';
</script>
<script type="text/javascript" src="{$this_path_ssl}views/js/jquery.blockUI.js"></script>
<script type="text/javascript" src="{$this_path_ssl}views/js/init.js"></script>

<div class="row">
<div class="col-xs-12 col-md-6">
<div class="panel panel-default" style="border-radius: 5px; background: #fbfbfb;">
{if !empty($titulo_pix)}
<div class="panel-heading">{$titulo_pix}</div>
{/if}
<div class="panel-body">
	
	<div id="form-paghiperpix">
	
	<div class="form-group">
		<div class="col-md-12 colunas_paghiperpix">
		<p style="margin-bottom: 10px;" class="form-control-static">
		<img style="max-width: 100% !important;" src="data:image/jpeg;base64,"/>
		</p>
		</div>
	</div>

	<div class="form-group">
		<div class="col-md-12 colunas_paghiperpix">
		<label>Pagador:</label>
		<input class="no-copy-paste campos_formatados"type="text" value="{$pagador}" size="50" readonly>
		</div>
	</div>
	
	{if $fiscal_size==11 || $fiscal_size==14}
		<input type="hidden" id="fiscal" name="fiscal" value="{$fiscal}">
	{else}
		<div class="form-group">
			<div class="col-md-12 colunas_paghiperpix">
			<label>CPF ou CNPJ do Pagador:</label>
			<input maxlength="14" placeholder="00000000000" class="no-copy-paste campos_formatados" onkeyup="mascara(this,'soNumeros')" type="text" id="fiscal" name="fiscal" value="{$fiscal}" size="50" required>
			</div>
		</div>
	{/if}
	
	{if $desconto_pix > 0}
	<div class="form-group">
		<div class="col-md-12 colunas_paghiperpix">
		<label>Total a Pagar:</label>
		<input class="no-copy-paste campos_formatados"type="text" value="R${$total_pix}{if $desconto_pix > 0} ({$desconto_pix}% de desconto){/if}" size="50" readonly>
		</div>
	</div>
	{/if}
	
	<div class="form-group">
		<div class="col-sm-12 colunas_paghiperpix">
		&nbsp;Ao finalizar ser&aacute; exibido o Pix para pagamento no App de seu banco preferido.
		</div>
	</div>

	<div class="form-group">
		<div class="col-sm-12 colunas_paghiperpix">
		<button onclick="processar_paghiperpix()" type="button" id="botao-paghiperpix" class="button btn btn-primary btn-lg pull-right">Pagar com Pix</button>
		</div>
	</div>

	</div>
	
	<script>
	//urls do sistema
	var url_modulo_pix = "{$url_loja}index.php?fc=module&module=paghiperpix&controller=confirmar&ajax=true";
	{literal}
	//funcoes
	function processar_paghiperpix(){
		var fiscal = $('#form-paghiperpix input[name="fiscal"]');
		if(!validarCpfCnpj(fiscal.val())){
			alert('Informe um CPF ou CNPJ de pagamento válido.');
			fiscal.focus();
			return false;
		}
		bloquear_tela();
		var form = $("#form-paghiperpix");
		var dados = form.serialize();
		document.getElementById('botao-paghiperpix').disabled=true;
		$.ajax({
			type: 'post',
			dataType: 'JSON',
			data: {fiscal: fiscal.val()},
			url: url_modulo_pix,
			cache: false,
			success: function(dados) {
				console.log(dados);
				if (dados.status==false) {
					alert(dados.erro);
					desbloquear_tela();
					document.getElementById('botao-paghiperpix').disabled=false;
				}else{
					location.href = dados.cupom;
				}
				return false;
			}
		});
		return false;
	}
	{/literal}
	</script>

</div>
</div>
</div>
</div>
